{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@nicepkg/translate-core#build": {
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "doc#build": {
      "inputs": ["./**"],
      "outputs": ["./.vuepress/dist/**"]
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig-base.json"]
}
